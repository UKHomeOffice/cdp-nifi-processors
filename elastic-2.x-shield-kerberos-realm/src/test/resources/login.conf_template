com.sun.security.jgss.krb5.initiate {
    com.sun.security.auth.module.Krb5LoginModule
    required
    refreshKrb5Config=true
    storeKey=false
    principal="${initiator.principal}"
    ticketCache="${initiator.ticketcache}"
    useTicketCache=true
    debug=${debug}
    ;
};

no.ticket.cache {
    com.sun.security.auth.module.Krb5LoginModule
    required
    refreshKrb5Config=true
    storeKey=false
    useTicketCache=false
    debug=${debug}
    ;
};